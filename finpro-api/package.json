{
  "name": "pwd-template-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "nodemon",
    "build": "prisma generate &&tsc && mkdir -p api && cp -r dist/src/* api/",
    "vercel-bulid": "prisma generate && tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:dev": "set PGOPTIONS='--statement_timeout=30000' && dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:studio": "dotenv -e .env.development -- npx prisma studio",
    "prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "prisma:format": "dotenv -e .env.development -- npx prisma format",
    "prisma:push": "dotenv -e .env.development -- npx prisma db push",
    "prisma:seed": "dotenv -e .env.development -- npx prisma db seed",
    "postinstall": "prisma generate && tsc"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.6.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "lint-staged": "^15.5.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
